{
  "name": "offline-checker-cron",
  "version": "1.0.0",
  "description": "Cronjob to check offline checker",
  "main": "dist/app.js",
  "scripts": {
    "start": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/app.ts",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json -t verbose",
    "serve": "node dist/app.js",
    "build": "npm run tslint && npm run build-ts"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "author": "Dhian Pratama",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/node": "^10.14.7",
    "@types/shelljs": "^0.8.3",
    "nodemon": "^1.18.10",
    "shelljs": "^0.8.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^6.0.0",
    "http-status": "^1.3.1",
    "moment": "^2.24.0",
    "node-graceful": "^0.3.1",
    "rxjs": "^5.5.2",
    "rxredis": "^0.0.4",
    "slug": "^1.1.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  }
}
